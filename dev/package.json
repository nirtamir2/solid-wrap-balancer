{
  "name": "solid-start-starter",
  "version": "0.0.0",
  "private": false,
  "description": "SolidJS project starter template with TypeScript, ESLint, TailwindCSS",
  "keywords": [
    "solid",
    "template",
    "solid-start",
    "solidjs"
  ],
  "homepage": "https://github.com/nirtamir2/solid-start-starter#readme",
  "bugs": {
    "url": "https://github.com/nirtamir2/solid-start-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirtamir2/solid-start-starter.git"
  },
  "license": "MIT",
  "author": "nirtamir2",
  "type": "module",
  "scripts": {
    "build": "solid-start build",
    "dev": "solid-start dev",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "postinstall": "node add-ts-nocheck.cjs",
    "lint": "eslint --max-warnings 0 --fix \"**/*.{ts,tsx,js,jsx}\"",
    "start": "solid-start start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.7.0",
    "copy-to-clipboard": "^3.3.3",
    "solid-devtools": "^0.24.7",
    "solid-js": "^1.6.10",
    "solid-start": "^0.2.19",
    "undici": "5.18.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@tsconfig/strictest": "^1.0.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-nirtamir2": "^0.0.47",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.4.2",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "solid-start-node": "^0.2.19",
    "solid-start-vercel": "^0.2.19",
    "solid-wrap-balancer": "^0.0.4",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-solid-svg": "^0.6.0"
  },
  "packageManager": "pnpm@7.5.0"
}
