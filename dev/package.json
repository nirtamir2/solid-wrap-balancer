{
  "name": "solid-start-starter",
  "version": "0.0.0",
  "private": false,
  "description": "SolidJS project starter template with TypeScript, ESLint, TailwindCSS",
  "keywords": [
    "solid",
    "template",
    "solid-start",
    "solidjs"
  ],
  "homepage": "https://github.com/nirtamir2/solid-start-starter#readme",
  "bugs": {
    "url": "https://github.com/nirtamir2/solid-start-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirtamir2/solid-start-starter.git"
  },
  "license": "MIT",
  "author": "nirtamir2",
  "type": "module",
  "scripts": {
    "build": "solid-start build",
    "dev": "solid-start dev",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "postinstall": "node add-ts-nocheck.cjs",
    "lint": "eslint --max-warnings 0 --fix \"**/*.{ts,tsx,js,jsx}\"",
    "start": "solid-start start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@solidjs/meta": "^0.28.5",
    "@solidjs/router": "^0.8.2",
    "copy-to-clipboard": "^3.3.3",
    "solid-devtools": "^0.27.3",
    "solid-js": "^1.7.6",
    "solid-start": "^0.2.26",
    "undici": "5.22.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/strictest": "^2.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-nirtamir2": "^0.0.52",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "solid-start-node": "^0.2.26",
    "solid-start-vercel": "^0.2.26",
    "solid-wrap-balancer": "^0.0.5",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-solid-svg": "^0.6.1"
  },
  "packageManager": "pnpm@8.6.2"
}
